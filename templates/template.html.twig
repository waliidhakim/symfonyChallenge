	<meta name="description" content=""/>
	<meta name="author" content=""/>
	<title>{% block title %}Template Title{% endblock %}</title>
	<link href="{{ asset('assets/css/styles.css') }}" rel="stylesheet"/>
	<link href="{{ asset('assets/css/myStyle.css') }}" rel="stylesheet"/>
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/5.15.4/css/all.min.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/select2/4.1.0-beta.1/css/select2.min.css">
	<link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/toastr.js/latest/toastr.min.css">
	<style>
		:root {
			--primary-color: rgba(19, 221, 222, 0.13);
			--secondary-color: #f2e1c1;
			--accent-color: #a1c4fd;
			--text-color: #333333;
		}

		#layoutSidenav_nav{
			background-color : var(--bs-card-bg)  !important;
		}

		#layoutSidenav_content{
			display: flex;
			justify-content: center;
			align-items: center;
		}

		#layoutSidenav_content main{
			width: 100%;
		}
		body {
			font-family: 'Poppins', sans-serif;
			color: var(--text-color);
			background: url('https://offloadmedia.feverup.com/parissecret.com/wp-content/uploads/2020/11/28050319/idees-cadeaux-culture-1024x554.jpg') no-repeat center center fixed;
			background-size: cover;
		}

		.content-wrapper {
			background: rgba(255, 255, 255, 0.8); /* Opacité réglée pour le contenu */
			padding: 20px;

		}

        .navbar, footer {
            background-color: var(--primary-color);
            color: white;
			border-radius: 10px;

        }

        .navbar a:hover, footer a:hover {
            color: var(--secondary-color);
        }

        button, input[type='submit'], input[type='text'], select {
            background-color: var(--secondary-color);
            color: var(--text-color);
            border: none;
            border-radius: 15px;
            padding: 10px 20px;
            margin: 5px;
            transition: background-color 0.3s ease;
        }

        button:hover, input[type='submit']:hover {
            background-color: var(--accent-color);
            color: white;
        }

        ::-webkit-scrollbar {
            width: 10px;
        }

        ::-webkit-scrollbar-track {
            background: var(--secondary-color);
        }

        ::-webkit-scrollbar-thumb {
            background: var(--primary-color);
        }

        ::-webkit-scrollbar-thumb:hover {
            background: var(--accent-color);
        }

        @media (max-width: 768px) {
            /* Responsive styles */
        }

        .card {
            background: rgba(255, 255, 255, 0.3);
            border: none;
        }

        .card-header, .card-footer {
            background: var(--primary-color);
            color: white;
        }

        .link, a {
            color: var(--accent-color);
            transition: color 0.3s ease;
        }

        a:hover {
            color: var(--secondary-color);
        }

        .profile-image {
            border-radius: 50%;
            border: 3px solid var(--secondary-color);
			height: 50px;
			width: 50px;
        }
    </style>
    <script src="https://use.fontawesome.com/releases/v6.3.0/js/all.js" crossorigin="anonymous"></script>
</head>
<body>
